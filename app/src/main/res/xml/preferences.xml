<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:defaultValue="0"
        android:dialogTitle="Odaberite kvalitet video snimka:"
        android:entries="@array/stream_quality_titles"
        android:entryValues="@array/stream_quality_values"
        android:key="streamQuality"
        android:summary="Odaberite kvalitet video snimka"
        android:title="Video kvalitet" />

    <ListPreference
        android:defaultValue="0"
        android:dialogTitle="Odaberite po훾etni ekran:"
        android:entries="@array/start_fragment_titles"
        android:entryValues="@array/start_fragment_values"
        android:key="startFragment"
        android:summary="Odaberite po훾etni ekran"
        android:title="Po훾etni ekran" />

    <Preference
        android:title="A탑urirajte liste"
        android:key="updateData"/>

</PreferenceScreen>